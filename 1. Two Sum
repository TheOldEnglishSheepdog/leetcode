class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        listi=[]
        for i in range(len(nums)):
            for j in range(i+1,len(nums),1):
                #print('i,j: ',i,j)
                if nums[i]+nums[j]==target:
                    listi.append(i)
                    listi.append(j)
        return listi
        
